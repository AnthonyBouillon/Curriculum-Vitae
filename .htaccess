Options +FollowSymlinks
RewriteEngine On
# Réécriture d'url
RewriteRule ^Contact$ Views/contact.php [L] 
RewriteRule ^Liste-des-projets$ Views/project.php [L] 
RewriteRule ^Certificats-d'openclassroom$ Views/openclassroom-certificate.php [L] 
RewriteRule ^Accueil$ index.php [L] 
RewriteRule ^Compétence-Formation-Experience$ Views/c-f-e.php [L] 
# Si erreur, redirection vers la page correspondant
ErrorDocument 400 /CV/Error-page/error.php?error=400
ErrorDocument 403 /CV/Error-page/error.php?error=403
ErrorDocument 404 /CV/Error-page/error.php?error=404
ErrorDocument 500 /CV/Error-page/error.php?error=500
ErrorDocument 501 /CV/Error-page/error.php?error=501
ErrorDocument 502 /CV/Error-page/error.php?error=502
ErrorDocument 503 /CV/Error-page/error.php?error=503
ErrorDocument 504 /CV/Error-page/error.php?error=504
ErrorDocument 505 /CV/Error-page/error.php?error=505
